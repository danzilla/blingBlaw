<%- include('../layout/header') -%>

<!-- Navbar TOP -->
<div class="row">
  <div class="col">
    <%- include('../layout/navBar') -%>
  </div>
</div>

<div class="col h-100">
  <div class="row h-100">

    <!-- Navbar Left side -->
    <div class="col-md-2 sideNavLeft h-100">
      <div class="row h-100">
        <%- include('sideLeft') -%>
      </div>
    </div>

    <!-- Content data -->
    <div class="col-md-8 h-100">
      <div class="container-fluid h-100">

        <!-- Content container -->
        <div class="overFlow-yes h-100">
          <div class="container-fluid mt-3">
            <div class="col">
              <h1 class="display-5 text-info"><%- pageInfo.page %><small><%- pageInfo.query %></small></h1>
              <p class="code"><%- data %></p>
            </div>
          </div>

          <div class="overFlow-yes h-75">
            <%- include('searchInfo') -%>
          </div>
        </div>

      </div>
    </div>

    <!-- Navbar Right side -->
    <div class="col-md-2 sideNavRight h-100">
      <div class="row h-100">
        <%- include('sideRight') -%>
      </div>
    </div>

  </div>
</div>

<%- include('../layout/footer') -%>
